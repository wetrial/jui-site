  "scripts": {
    "start": "cross-env NODE_ENV=development bisheng start -c ./site/bisheng.config.js",
    "bisheng-build": "cross-env NODE_ENV=production bisheng build -c ./site/bisheng.config.js",
    "build-site": "rm -rf _site && npm run bisheng-build",
    "gh-pages": "gh-pages -d _site",
    "site": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npm run build-site && npm run gh-pages",
    "patch": "yarn version --patch && git push origin master && git push origin --tags",
    "minor": "yarn version --minor && git push origin master && git push origin --tags",
    "major": "yarn version --major && git push origin master && git push origin --tags"
  }
